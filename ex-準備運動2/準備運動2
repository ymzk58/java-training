import java.util.Scanner;

public class Main{
    public static void main(String[] args){
        System.out.print("数値を入力 >");
        int num = new java.util.Scanner(System.in).nextInt();
        for(int i = 0; i < num; i++){
            System.out.println((i+1) + ": Hello,World!");
        }
    }
}