public class Main {
    public static void main(String[] args){
        double pi = 3.14;   //π
        int pie = 5;        //パイの半径
        System.out.println("半径"+pie+"cmのパイの面積は、");
        System.out.println(pie*pie*pi);
        System.out.println("半径を倍にします");
        pi = 10;            //誤った代入
        
        System.out.println("半径"+pie+"cmのパイの面積は、");
        System.out.println(pie*pie*pi);
    }
}
